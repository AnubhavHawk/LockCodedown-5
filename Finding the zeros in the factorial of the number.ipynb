    "def findZeroes(n):\n",
    "    ans=0\n",
    "    p=5\n",
    "    while((n//p)>0):\n",
    "        ans+=(n//p)\n",
    "        p=p*5\n",
    "    return ans\n",
    "\n",
    "if __name__ == \"__main__\":\n",
    "    n=int(input(\"enter the value:\"))\n",
    "    print(findZeroes(n))"
   ]
  }
